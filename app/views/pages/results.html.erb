<h1 class="results">Results</h1>

<div class="events-pages-info">
  <%@event.each do |e| %>
    <h3><%=link_to "#{e.name}", gym_event_path(@gym, e) %></h3>
    <p>Event: <em><%= e.event_type %></em></p>
    <p>Start: <%= e.start_date.strftime("%b. %e, %Y") %>
      - End: <%= e.end_date.strftime("%b. %e, %Y") %></p>
  <% end %>
</div>


